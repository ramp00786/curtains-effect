{
  "name": "curtain-opening-effect",
  "version": "1.0.4",
  "description": "Beautiful curtain opening animation for websites - supports Vanilla JS, React, Vue, and Web Components",
  "main": "dist/vanilla/js/curtain-effect.js",
  "module": "dist/vanilla/js/curtain-effect.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/vanilla/js/curtain-effect.js",
      "require": "./dist/vanilla/js/curtain-effect.js"
    },
    "./vanilla": {
      "types": "./types/index.d.ts",
      "import": "./dist/vanilla/js/curtain-effect.js",
      "require": "./dist/vanilla/js/curtain-effect.js"
    },
    "./react": {
      "types": "./types/index.d.ts",
      "import": "./dist/react/index.jsx",
      "require": "./dist/react/index.jsx"
    },
    "./vue": {
      "types": "./types/index.d.ts",
      "import": "./dist/vue/index.js",
      "require": "./dist/vue/index.js"
    },
    "./web-component": {
      "types": "./types/index.d.ts",
      "import": "./dist/web-component/curtain-effect.esm.js",
      "require": "./dist/web-component/curtain-effect.js"
    },
    "./css": "./dist/vanilla/css/curtain-effect.css",
    "./react/css": "./dist/react/CurtainEffect.css"
  },
  "files": [
    "dist/",
    "types/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:vanilla && npm run build:react && npm run build:vue && npm run build:web-component",
    "build:vanilla": "echo 'Vanilla build complete'",
    "build:react": "echo 'React build complete'",
    "build:vue": "echo 'Vue build complete'",
    "build:web-component": "echo 'Web Component build complete'",
    "test": "echo 'Tests would go here'",
    "dev": "python -m http.server 8000",
    "demo": "start http://localhost:8000/curtain-examples.html"
  },
  "keywords": [
    "curtain",
    "animation",
    "effect",
    "opening",
    "vanilla-js",
    "react",
    "vue",
    "web-components",
    "css",
    "javascript",
    "ui",
    "transition",
    "entrance",
    "splash",
    "intro"
  ],
  "author": {
    "name": "Tulsiram Kushwah",
    "url": "https://www.linkedin.com/in/tulsiram-kushwah-software-engineer/",
    "social": {
      "linkedin": "https://www.linkedin.com/in/tulsiram-kushwah-software-engineer/",
      "facebook": "https://www.facebook.com/ramp00786"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramp00786/curtains-effect.git"
  },
  "bugs": {
    "url": "https://github.com/ramp00786/curtains-effect/issues"
  },
  "homepage": "https://github.com/ramp00786/curtains-effect#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "typescript": "^4.9.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "publishConfig": {
    "access": "public"
  }
}
